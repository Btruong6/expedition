{
  "name": "expedition-quest-creator",
  "version": "1.6.7",
  "description": "The Expedition Quest Creator",
  "homepage": "http://quests.expeditiongame.com",
  "scripts": {
    "lint": "tslint -c tslint.json 'app/**/*.{ts,tsx}'",
    "test": "mocha meta_tests.js && karma start karma.conf.js --single-run --browsers PhantomJS",
    "watch:test": "karma start karma.conf.js",
    "dev": "webpack-dev-server --progress",
    "start": "node app.js",
    "build": "webpack -p --config webpack.dist.config.js --progress"
  },
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "ace": "^1.3.0",
    "brace": "^0.10.0",
    "cheerio": "^0.22.0",
    "html": "^1.0.0",
    "joi-browser": "^10.0.6",
    "markdown": "^0.5.0",
    "mathjs": "^3.16.4",
    "numeral": "^2.0.6",
    "query-string": "^4.3.4",
    "react-ace": "^4.1.6",
    "react-split-pane": "^0.1.59",
    "redux-thunk": "^2.2.0",
    "typo-js": "^1.0.3",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/cheerio": "^0.22.1",
    "@types/jasmine": "^2.5.45",
    "@types/jquery": "^2.0.41",
    "@types/promise-polyfill": "~6.0.0",
    "@types/react-truncate": "^2.3.2",
    "@types/semver": "^5.5.0",
    "@types/stripe-v3": "^3.0.5",
    "babel-core": "~6.26.3",
    "babel-loader": "~6.4.1",
    "babel-polyfill": "~6.22.0",
    "css-loader": "^0.28.11",
    "jasmine": "~2.5.3",
    "karma": "^2.0.2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.2",
    "phantomjs-polyfill-object-assign": "~0.0.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.2"
  }
